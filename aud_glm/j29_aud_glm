#!/bin/bash


echo started on $HOSTNAME 
 date

tic="$(date +%s)"


 /network/lustre/iss01/apps/lang/matlab/R2020b/bin/matlab -singleCompThread -nodesktop  -nosoftwareopengl -r "run('/network/lustre/iss02/cohen/data/Fabien_official/SYNESTHEX/scripts/aud_glm/mfonc_j29_aud_glm')"


toc="$(date +%s)";
sec="$(expr $toc - $tic)";
min="$(expr $sec / 60)";
heu="$(expr $sec / 3600)";
echo Elapsed time: $min min $heu H
